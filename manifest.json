{
  "manifest_version": 3,
  "name": "Threat Detector",
  "version": "1.0",
  "description": "Detects and alerts about malicious websites.",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    
    "default_popup": "popup_safe.html",
    "default_icon": {
      "16": "icons/safe16.png",
      "48": "icons/safe48.png",
      "128": "icons/safe128.png"
    }
  },
  "icons": {
    "16": "icons/safe16.png",
    "48": "icons/safe48.png",
    "128": "icons/safe128.png"
  }
}